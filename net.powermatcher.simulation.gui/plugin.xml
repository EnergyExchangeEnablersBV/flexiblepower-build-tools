      <command
            defaultHandler="net.powermatcher.simulation.gui.handlers.NewScenarioWizard"
            id="net.powermatcher.simulation.gui.command.newscenariowizard"
            name="New Scenario Wizard">
      </command>
      <command
            id="net.powermatcher.simulation.gui.command.delete"
            name="Delete">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="net.powermatcher.simulation.gui.handlers.Save"
            commandId="org.eclipse.ui.file.save">
      </handler>
      <handler
            class="net.powermatcher.simulation.gui.handlers.Undo"
            commandId="org.eclipse.ui.edit.undo">
      </handler>
      <handler
            class="net.powermatcher.simulation.gui.handlers.Redo"
            commandId="org.eclipse.ui.edit.redo">
      </handler>
      <handler
            class="net.powermatcher.simulation.gui.handlers.Delete"
            commandId="org.eclipse.ui.edit.delete">
      </handler>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="NodeSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.NodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="AuctioneerSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.AuctioneerNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="ConcentratorSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.ConcentratorNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="ClusterSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.ClusterDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="ScenarioSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.ScenarioDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="DeviceAgentSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.DeviceAgentNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="FPAIDeviceAgentSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.FPAIDeviceAgentNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="FPAIDeviceAgentSelectedWithoutChildren">
         <iterate
               ifEmpty="true">
            <instanceof
                  value="net.powermatcher.simulation.configuration.FPAIDeviceAgentNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="ResourceManagerSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.ResourceManagerNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="ResourceModelSelected">
         <iterate
               ifEmpty="false">
            <instanceof
                  value="net.powermatcher.simulation.configuration.ResourceDriverNodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="SomethingSelected">
         <iterate
               ifEmpty="false"
               operator="or">
            <instanceof
                  value="net.powermatcher.simulation.configuration.ScenarioDescriptor">
            </instanceof>
            <instanceof
                  value="net.powermatcher.simulation.configuration.ClusterDescriptor">
            </instanceof>
            <instanceof
                  value="net.powermatcher.simulation.configuration.NodeDescriptor">
            </instanceof>
         </iterate>
      </definition>
      <definition
            id="OpenEditorDefinition">
         <iterate
               ifEmpty="false"
               operator="or">
            <or>
               <instanceof
                     value="net.powermatcher.simulation.configuration.NodeDescriptor">
               </instanceof>
               <instanceof
                     value="net.powermatcher.simulation.configuration.ScenarioDescriptor">
               </instanceof>
            </or>
         </iterate>
      </definition>
      <definition
            id="AddAgentDefinition">
         <iterate
               ifEmpty="false"
               operator="or">
            <or>
               <instanceof
                     value="net.powermatcher.simulation.configuration.AuctioneerNodeDescriptor">
               </instanceof>
               <instanceof
                     value="net.powermatcher.simulation.configuration.ConcentratorNodeDescriptor">
               </instanceof>
            </or>
         </iterate>
      </definition>
      <definition
            id="ConcDeviceAgentDefinition">
         <iterate
               ifEmpty="false"
               operator="or">
            <and>
               <instanceof
                     value="net.powermatcher.simulation.configuration.NodeDescriptor">
               </instanceof>
               <not>
                  <instanceof
                        value="net.powermatcher.simulation.configuration.AuctioneerNodeDescriptor">
                  </instanceof>
               </not>
            </and>
         </iterate>
      </definition>
      <definition
            id="SupportsBidAndPriceGraph">
         <iterate
               ifEmpty="false"
               operator="or">
            <or>
               <instanceof
                     value="net.powermatcher.simulation.configuration.AuctioneerNodeDescriptor">
               </instanceof>
               <instanceof
                     value="net.powermatcher.simulation.configuration.ConcentratorNodeDescriptor">
               </instanceof>
               <instanceof
                     value="net.powermatcher.simulation.configuration.DeviceAgentNodeDescriptor">
               </instanceof>
               <instanceof
                     value="net.powermatcher.simulation.configuration.FPAIDeviceAgentNodeDescriptor">
               </instanceof>
            </or>
         </iterate>
      </definition>
   </extension>
