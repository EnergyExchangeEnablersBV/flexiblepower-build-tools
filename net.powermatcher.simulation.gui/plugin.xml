   <extension
         point="org.eclipse.ui.bindings">
      <scheme
            id="PowerMatcherSimulatorScheme"
            name="PowerMatcherSimulatorScheme"
            parentId="org.eclipse.ui.defaultAcceleratorConfiguration">
